<% content_for :main do %>
<div class="panel panel-default">
  <div class="panel-heading">
    Editar cafetería
    <span class="pull-right">
      <%= link_to 'Cancelar', cafeterias_path %> |
      <%= link_to 'Eliminar', @cafeteria, method: :delete, data: { confirm: 'Estás seguro?' } %>
    </span>
  </div>
  <div class="panel-body">
    <%= render 'form', cafeteria: @cafeteria %>
  </div>
</div>
<% end %>
